import matplotlib.pyplot as plt

# Data
teams = ["Men's Swimming", "Men's Volleyball", "Women's Swimming", "Women's Volleyball"]
heights = [71.23, 73.24, 71.23, 71.23]  # Replace with your actual values if different

# Create bar chart
plt.bar(teams, heights)
plt.ylabel("Average Height (inches)")
plt.title("Average Height by Team")
plt.xticks(rotation=45)
plt.ylim(0, 80)  # Optional: sets y-axis range
plt.show()
